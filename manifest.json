{	"name": "dontletmegothere",
	"description": "Extension that helps you not spend your time on social media",
	"version": "1.0",
	"manifest_version": 2,
	"permissions": ["tabs", "activeTab", "storage", "http://*/*", "https://*/*"],
	"background": {
		"scripts": ["background.js"],
		"persistent": false
	},
	"browser_action": {
		"default_popup": "popup.html",
		"default_icon": "icon.png"
	},
	"content_scripts": [{
	     "matches": ["<all_urls>"],
	     "js": ["contentScript.js"]
	   }
	 ]
}